  "version": "1.0.0",
  "description": "A database and API for storing information about stirkes, demonstrations and boycotts.",
  "dependencies": {
    "async": "^0.9.0",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "~1.0.1",
    "connect-flash": "~0.1.1",
    "cookie-parser": "~1.0.0",
    "ejs": "1.0.0",
    "express": "~4.0.0",
    "express-jwt": "^0.3.2",
    "express-session": "~1.0.0",
    "jshint": "^2.6.3",
    "jsonwebtoken": "^1.1.2",
    "jwt-simple": "^0.2.0",
    "lodash": "^2.4.1",
    "method-override": "~1.0.0",
    "moment": "^2.8.3",
    "mongoose": "~3.6.13",
    "morgan": "~1.0.0",
    "node-sass": "^2.0.1",
    "passport": "~0.1.17",
    "passport-facebook": "~1.0.2",
    "passport-google-oauth": "~0.1.5",
    "passport-local": "~0.1.6",
    "passport-twitter": "~1.0.2",
    "path": "^0.4.9",
    "pg": "^3.4.1",
    "sequelize": "^1.7.9",
    "uglify-js": "2.4.16",
    "underscore": "1.7.0"
  },
  "repository": {
    "type": "github",
    "url": "//github.com/nanoo-k/StrikeBack"
  },
  "scripts": {
    "env": "env",
    "sass-foundation": "node-sass app/stylesheets/app.scss app/web/stylesheets/app.css",
    "app-uglify": "uglifyjs app/js/strikestarter/*.js app/js/strikestarter/**/*.js -o app/web/javascripts/app_compiled.js -beautify",
    "cp-templates": "for file in $(find app/js/strikestarter/**/*.html -type f -exec basename {} \\;); do cp app/js/strikestarter/**/$file app/web/templates/$file; done;"
